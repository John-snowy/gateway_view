   <chart height=\"100%\" width=\"100%\" :data=\"chartData\" />\n  </div>\n</template>\n\n<script>\nimport Chart from './components/LineStat'\nimport { appStat, appDetail } from '@/api/app'\n\nexport default {\n  name: 'AppStat',\n  components: { Chart },\n  data() {\n    return {\n      chartData: {\n        'title': '',\n        'today': [],\n        'yesterday': []\n      }\n    }\n  },\n  created() {\n    const id = this.$route.params && this.$route.params.id\n    this.fetchStat(id)\n  },\n  methods: {\n    fetchStat(id) {\n      const query = { 'id': id }\n      appStat(query).then(response1 => {\n        appDetail(query).then(response2 => {\n          this.chartData = {\n            'title': response2.data.name + '租户统计',\n            'today': response1.data.today,\n            'yesterday': response1.data.yesterday\n          }\n          console.log(this.chartData)\n        })\n      }).catch(() => {\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.chart-container{\n  position: relative;\n  width: 100%;\n  height: calc(100vh - 84px);\n}\n</style>\n"],"sourceRoot":"src/views/app"}]}